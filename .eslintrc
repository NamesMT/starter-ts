{
  "root": true,
  "extends": [
    "prettier",
    "turbo",
    "@antfu",
    "@unocss"
  ],
  "rules": {
    "indent": 0,
    "max-statements-per-line": [
      1,
      {
        "max": 3
      }
    ],
    "react/no-unknown-property": 0,
    "import/no-mutable-exports": 0,
    "no-console": 1,
    "eqeqeq": 1
  }
}
