{
  "name": "starter-ts",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.10.2",
  "description": "",
  "author": "NamesMT <dangquoctrung123@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/namesmt",
  "homepage": "https://github.com/namesmt/starter-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namesmt/starter-ts.git"
  },
  "bugs": "https://github.com/namesmt/starter-ts/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "stub": "unbuild --stub",
    "dev": "pnpm run watch",

    "lint": "eslint .",
    "test": "vitest",
    "test:types": "tsc --noEmit --skipLibCheck",
    "check": "pnpm lint && pnpm test:types && vitest run --coverage",

    "build": "unbuild",
    "release": "pnpm dlx changelogen@latest --release --push --publish",

    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "pino": "^8.16.1",
    "readable-http-codes": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@types/node": "^20.8.10",
    "@unocss/eslint-config": "^0.57.1",
    "@vitest/coverage-v8": "^0.34.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "lint-staged": "^15.0.2",
    "pino-pretty": "^10.2.3",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "function-bind": "npm:@nolyfill/function-bind@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.values": "npm:@nolyfill/object.values@latest"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
